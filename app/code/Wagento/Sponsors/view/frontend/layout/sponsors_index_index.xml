<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceBlock name="content">
      <block class="Wagento\Sponsors\Block\Sponsors" name="sponsors" template="Wagento_Sponsors::list.phtml" />
      <container name="container.cms" htmlTag="div" htmlClass="container-information">
        <block class="Magento\Cms\Block\Block" name="sponsors.info">
          <arguments>
            <argument name="block_id" xsi:type="string">sponsors_info</argument>
          </arguments>
        </block>
      </container>
    </referenceBlock>
  </body>
</page>
