<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="top.links">
            <block class="Wagento\Sponsors\Block\Sponsors" name="sponsors-top-link" before="my-account-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Sponsors</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="header.links">
            <block class="Wagento\Sponsors\Block\Sponsors" name="sponsors-header-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Sponsors</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
